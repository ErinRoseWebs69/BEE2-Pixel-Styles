LightMappedGeneric
{
$basetexture "metal/metal_lift001_gradient"

$envmap "BEE2/p1/metal_lift_cube"
// Selfillum doesn't seem to work with $basealphaenvmapmask, so use a separate texture
$envmapmask "BEE2/p1/metal_lift_grad_mask"
$selfillum 1

// Darken the reflection in darker areas
$envmaplightscale 1

$surfaceprop metal
"%keywords" "BEE2 P1"
"%noportal" 1
%nopaint 1
$envmaptint "[ .9 .9 .9 ]"

}
